    "alchemy-sdk": "^3.3.1",
    "axios": "^1.6.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ip": "^1.0.4",
    "express-rate-limit": "^7.1.4",
    "jsonwebtoken": "^8.5.1",
    "moralis": "^2.26.2",
    "mssql": "^10.0.1",
    "node-binance-api": "^0.13.1",
    "node-telegram-bot-api": "^0.64.0",
